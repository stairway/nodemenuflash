package com.nodemenu {		import flash.events.Event;		public class NodeEvent extends Event {				public static const MENU_CLOSED:String = "menuClosed";		public static const MENU_OPEN:String = "menuOpen";		public static const NODE_OPEN:String = "nodeOpen";		public static const NODE_CLOSED:String = "nodeClosed";		public static const XML_LOADED:String = "xmlLoaded";				private var _nodeName:String;			// Initialization:		public function NodeEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false, nodeName:String=null) {						super(type, bubbles, cancelable);            this._nodeName = nodeName;		}				public function get nodeName():String {            return _nodeName;        }         public function set nodeName(value:String):void {            _nodeName = value;        }				override public function toString():String {            return formatToString("NodeEvent", "type", "bubbles", "cancelable", "eventPhase", "nodeName");        }				override public function clone():Event {            return new NodeEvent(type, bubbles, cancelable, nodeName);        }	}}